{
  "expo": {
    "name": "品評会",
    "slug": "hinpyoukai",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/logo-sm.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 10000,
      "url": "https://u.expo.dev/9a87a3a9-f7bd-4ba3-8a22-4ee1157c0c1d"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "net.votepurchase.hinpyoukai",
      "buildNumber": "4",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "このアプリはマイクを使用します",
        "NSCameraUsageDescription": "このアプリは画像解析のために写真ライブラリを使用します",
        "NSPhotoLibraryAddUsageDescription": "このアプリは画像を保存するために写真ライブラリを使用します",
        "NSPhotoLibraryUsageDescription": "このアプリは画像解析のために写真ライブラリを使用します",
        "CFBundleDevelopmentRegion": "ja_JP"
      },
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "package": "net.votepurchase.hinpyoukai",
      "versionCode": 4,
      "userInterfaceStyle": "automatic",
      "permissions": [
        "VIBRATE",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "svg",
        "ts",
        "tsx"
      ]
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "9a87a3a9-f7bd-4ba3-8a22-4ee1157c0c1d"
      }
    }
  }
}
